{
  "buildCommand": "cd todo-react-frontend && npm install && npm run build",
  "outputDirectory": "todo-react-frontend/dist",
  "framework": null,
  "rewrites": [
    {
      "source": "/api-mongo/(.*)",
      "destination": "https://backend-express.conference-ticket-guilhermerios.tech/api/$1"
    },
    {
      "source": "/api-pg/(.*)",
      "destination": "https://atv2-postgre.conference-ticket-guilhermerios.tech/api/$1"
    },
    {
      "source": "/api/(.*)",
      "destination": "https://backend-express.conference-ticket-guilhermerios.tech/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}
